<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.textAML3.resource.textAML3.mopp.TextAML3MetaInformation" id="org.emftext.language.textAML3.resource.textAML3.mopp.TextAML3MetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension id="org.emftext.language.textAML3.resource.textAML3.problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.eclipse.core.resources.problemmarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
		<super type="org.eclipse.emf.ecore.diagnostic">
		</super>
	</extension>
	
	<extension id="org.emftext.language.textAML3.resource.textAML3.problem.syntax_error" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.textAML3.resource.textAML3.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.textAML3.resource.textAML3.problem.print_problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.textAML3.resource.textAML3.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.textAML3.resource.textAML3.problem.unresolved_reference" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.textAML3.resource.textAML3.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.textAML3.resource.textAML3.problem.analysis_problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.textAML3.resource.textAML3.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.textAML3.resource.textAML3.problem.builder_error" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.textAML3.resource.textAML3.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.textAML3.resource.textAML3.nature" name="textAML3 nature" point="org.eclipse.core.resources.natures">
		<runtime>
			<run class="org.emftext.language.textAML3.resource.textAML3.mopp.TextAML3Nature">
			</run>
		</runtime>
		<builder id="org.emftext.language.textAML3.resource.textAML3.builder">
		</builder>
	</extension>
	
	<extension point="org.eclipse.core.resources.builders" id="org.emftext.language.textAML3.resource.textAML3.builder" name="textAML3 Builder">
		<builder hasNature="true">
			<run class="org.emftext.language.textAML3.resource.textAML3.mopp.TextAML3BuilderAdapter">
			</run>
		</builder>
	</extension>
	
	<extension-point id="org.emftext.language.textAML3.resource.textAML3.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd">
	</extension-point>
	
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.textAML3.resource.textAML3.mopp.TextAML3ResourceFactoryDelegator" type="textAML3">
		</parser>
	</extension>
	
	<extension-point id="org.emftext.language.textAML3.resource.textAML3.additional_extension_parser" name="Additional Extension Parser" schema="schema/additional_extension_parser.exsd">
	</extension-point>
	
	<extension point="org.eclipse.debug.core.breakpoints">
		<breakpoint class="org.emftext.language.textAML3.resource.textAML3.debug.TextAML3LineBreakpoint" id="org.emftext.language.textAML3.resource.textAML3.debug.breakpoint" markerType="org.emftext.language.textAML3.resource.textAML3.debug.lineBreakpoint.marker" name="textAML3 Breakpoint">
		</breakpoint>
	</extension>
	
	<extension id="org.emftext.language.textAML3.resource.textAML3.debug.lineBreakpoint.marker" point="org.eclipse.core.resources.markers">
		<super type="org.eclipse.debug.core.lineBreakpointMarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
		<persistent value="true">
		</persistent>
	</extension>
	
	<extension point="org.eclipse.debug.core.launchConfigurationTypes">
		<launchConfigurationType id="org.emftext.language.textAML3.resource.textAML3.ui.launchConfigurationType" delegate="org.emftext.language.textAML3.resource.textAML3.launch.TextAML3LaunchConfigurationDelegate" modes="run,debug" name="textAML3 Application" sourceLocatorId="org.emftext.language.textAML3.resource.textAML3.ui.sourceLocator" sourcePathComputerId="org.emftext.language.textAML3.resource.textAML3.ui.sourcePathComputer" delegateName="textAML3 Launch Tooling" delegateDescription="This will run or debug .textAML3 files.">
		</launchConfigurationType>
	</extension>
	
	<extension point="org.eclipse.debug.core.sourcePathComputers">
		<sourcePathComputer class="org.emftext.language.textAML3.resource.textAML3.debug.TextAML3SourcePathComputerDelegate" id="org.emftext.language.textAML3.resource.textAML3.ui.sourcePathComputer">
		</sourcePathComputer>
	</extension>
	
	<extension point="org.eclipse.debug.core.sourceLocators">
		<sourceLocator id="org.emftext.language.textAML3.resource.textAML3.ui.sourceLocator" class="org.emftext.language.textAML3.resource.textAML3.debug.TextAML3SourceLocator" name="textAML3 Source Locator">
		</sourceLocator>
	</extension>
	
</plugin>
